    <declare-styleable name="BubbleRangeSeekBar">
        <attr name="max" format="float"/>
        <attr name="min" format="float"/>
        <attr name="reserve" format="float"/>
        <attr name="cells" format="integer"/>
        <attr name="lineColorSelected" format="color"/>
        <attr name="lineColorEdge" format="color"/>
        <attr name="seekBarResId" format="reference"/>
        <attr name="seekBarPressResId" format="reference"/>
        <attr name="bsb_bubble_text_color" format="color|reference"/> <!--text color of bubble-progress, default: #ffffffff-->
        <attr name="bsb_bubble_text_size" format="dimension|reference"/> <!--text size of bubble-progress, default: 14sp-->
        <attr name="bsb_bubble_color" format="color|reference"/> <!--color of bubble, default: same as left-track's color-->
        <attr name="bsb_second_track_color" format="color|reference"/> <!--color of left-track, default: R.color.colorAccent-->
        <attr name="bsb_bubble_drop_color" format="color|reference"/>
        <attr name="bsb_bubble_drop_size" format="integer|dimension|reference" />
        <attr name="bsb_bubble_drop_resid" format="reference"/>
    </declare-styleable>
